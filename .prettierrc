{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 4,
	"semi": false,
	"trailingComma": "none",
	"singleQuote": true,
	"printWidth": 100,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"useTabs": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"jsxBracketSameLine": false,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css"
}